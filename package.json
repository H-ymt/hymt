{
  "name": "hymt",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.18.3",
  "scripts": {
    "dev": "next dev",
    "prebuild": "pnpm fetch-knowledge || echo 'Warning: fetch-knowledge failed, continuing with existing entries.json'",
    "build": "pnpm fetch-knowledge || echo 'Warning: fetch-knowledge failed, continuing with existing entries.json' && next build",
    "build:cloudflare": "pnpm fetch-knowledge || echo 'Warning: fetch-knowledge failed, continuing with existing entries.json' && opennextjs-cloudflare build",
    "start": "next start",
    "lint": "next lint",
    "deploy": "pnpm build:cloudflare && opennextjs-cloudflare deploy",
    "preview": "pnpm build:cloudflare && opennextjs-cloudflare preview",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv ./cloudflare-env.d.ts",
    "fetch-knowledge": "tsx scripts/fetch-knowledge.ts"
  },
  "dependencies": {
    "@opennextjs/cloudflare": "^1.3.0",
    "@tanstack/react-query": "^5.90.6",
    "next": "15.4.6",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^24.3.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "tsx": "^4.20.6",
    "typescript": "^5",
    "wrangler": "^4.33.1"
  }
}
